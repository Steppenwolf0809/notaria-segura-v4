{
  "name": "notaria-segura-backend",
  "version": "1.0.0",
  "description": "API Backend para Sistema Notaría Segura",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "npx prisma db push --accept-data-loss && node prisma/seed.js && node server.js",
    "build": "npx prisma generate",
    "start:prod": "npx prisma generate && npx prisma db push --accept-data-loss && node server.js",
    "dev": "nodemon server.js",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push --accept-data-loss",
    "db:studio": "npx prisma studio",
    "db:seed": "node prisma/seed.js",
    "populate-users": "node scripts/populate-users.js",
    "seed:users": "node scripts/seed-real-users.js",
    "setup:prod": "node scripts/setup-production-data.js",
    "verify:prod": "node scripts/verify-production.js",
    "verify:users": "node scripts/verify-users.js",
    "fix:enum": "node scripts/fix-agrupado-enum.js",
    "fix:grouped": "node scripts/fix-grouped-documents-status.js smart",
    "fix:grouped-force": "node scripts/fix-grouped-documents-status.js force EN_PROCESO",
    "db:sync": "npx prisma db push --accept-data-loss"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "chokidar": "^3.6.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.0",
    "compression": "^1.8.1",
    "cors": "^2.8.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.0",
    "libphonenumber-js": "^1.12.10",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.0",
    "twilio": "^4.19.0",
    "xml2js": "^0.6.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "prisma": "^5.7.0"
  },
  "keywords": [
    "api",
    "notaria",
    "express",
    "postgresql"
  ],
  "author": "Notaría Segura",
  "license": "PRIVATE"
}
