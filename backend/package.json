{
  "name": "notaria-segura-backend",
  "version": "1.0.0",
  "description": "API Backend para Sistema Notaría Segura",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "build": "prisma generate && prisma db deploy",
    "start:prod": "prisma generate && prisma db deploy && node server.js",
    "postdeploy": "node ../scripts/setup-production-data.js",
    "dev": "nodemon server.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma db deploy",
    "db:studio": "prisma studio",
    "setup:prod": "node ../scripts/setup-production-data.js",
    "verify:prod": "node ../scripts/verify-production.js"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.0",
    "compression": "^1.8.1",
    "cors": "^2.8.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "twilio": "^4.19.0",
    "xml2js": "^0.6.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "prisma": "^5.7.0"
  },
  "keywords": [
    "api",
    "notaria",
    "express",
    "postgresql"
  ],
  "author": "Notaría Segura",
  "license": "PRIVATE"
}
